<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor
    name="edu_sharing-project"
    xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0
                        http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd">
  <requiredProperties>
    <requiredProperty key="groupId">
      <defaultValue>org.edu_sharing</defaultValue>
    </requiredProperty>
    <requiredProperty key="artifactId">
      <defaultValue>edu_sharing-projects-community</defaultValue>
    </requiredProperty>
    <requiredProperty key="version">
      <defaultValue>git</defaultValue>
    </requiredProperty>
    <requiredProperty key="project">
      <defaultValue>community</defaultValue>
    </requiredProperty>
    <requiredProperty key="REPOSITORY_ANTIVIRUS_ENABLED">
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="REPOSITORY_CLUSTER_ENABLED">
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="REPOSITORY_ELASTIC_ENABLED">
      <defaultValue>true</defaultValue>
    </requiredProperty>
    <requiredProperty key="REPOSITORY_KAFKA_ENABLED">
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="REPOSITORY_MONGO_ENABLED">
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="REPOSITORY_REMOTE_ENABLED">
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="REPOSITORY_TRANSFORM_ENABLED">
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="SERVICE_CONNECTOR_ENABLED">
      <defaultValue>false</defaultValue>
    </requiredProperty>
    <requiredProperty key="gitignore">
      <defaultValue>.gitignore</defaultValue>
    </requiredProperty>
    <requiredProperty key="gitattributes">
      <defaultValue>.gitattributes</defaultValue>
    </requiredProperty>
  </requiredProperties>
  <fileSets>
    <fileSet encoding="UTF-8">
      <directory>.mvn</directory>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>scripts</directory>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>__gitattributes__</include>
        <include>__gitignore__</include>
        <include>LICENSE.txt</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8" filtered="true">
      <directory></directory>
      <includes>
        <include>.gitlab-ci.yml</include>
        <include>README.md</include>
      </includes>
    </fileSet>
  </fileSets>
  <modules>
    <module dir="deploy"
            id="${rootArtifactId}-deploy"
            name="${rootArtifactId}-deploy">
      <fileSets>
        <fileSet encoding="UTF-8" filtered="true">
          <directory></directory>
          <includes>
            <include>.info</include>
          </includes>
        </fileSet>
      </fileSets>
      <modules>
        <module dir="docker"
                id="${rootArtifactId}-deploy-docker"
                name="${rootArtifactId}-deploy-docker">
          <fileSets>
            <fileSet encoding="UTF-8">
              <directory></directory>
              <includes>
                <include>.env.sample</include>
                <include>deploy.sh</include>
              </includes>
            </fileSet>
            <fileSet encoding="UTF-8" filtered="true">
              <directory></directory>
              <includes>
                <include>README.md</include>
              </includes>
            </fileSet>
          </fileSets>
          <modules>
            <module dir="build"
                    id="${rootArtifactId}-deploy-docker-build"
                    name="${rootArtifactId}-deploy-docker-build">
              <modules>
                <module dir="repository-service"
                        id="${rootArtifactId}-deploy-docker-build-repository-service"
                        name="${rootArtifactId}-deploy-docker-build-repository-service">
                  <fileSets>
                    <fileSet encoding="UTF-8">
                      <directory>src</directory>
                      <includes>
                        <include>**/*</include>
                      </includes>
                    </fileSet>
                  </fileSets>
                </module>

                <module dir="transform-edu-sharing"
                        id="${rootArtifactId}-deploy-docker-build-transform-edu-sharing"
                        name="${rootArtifactId}-deploy-docker-build-transform-edu-sharing">
                  <fileSets>
                    <fileSet encoding="UTF-8">
                      <directory>src</directory>
                      <includes>
